<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<!-- mapping URL -->

	<!-- Declare Servlet name -->
	<servlet>
		<servlet-name>TouristAttraction</servlet-name>
		<servlet-class>booking.controller.TouristAttraction</servlet-class>
	</servlet>
	<!-- Mapping with URL -->
	<servlet-mapping>
		<servlet-name>TouristAttraction</servlet-name>
		<url-pattern>/TouristAttraction</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
